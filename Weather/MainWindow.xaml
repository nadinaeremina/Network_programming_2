        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Weather"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBox TextWrapping="Wrap" x:Name="OutPutTB" Height="277"></TextBox>
            <Button Click="Button_Click">Weather</Button>
            <Button Click="Button_Click_1" >Weather_JS</Button>
        </StackPanel>
    </Grid>
</Window>
